body {
  padding: 0;
}

/* Undo overview background and borders */
#main #overview {
  background: #fff;
  border: none;
}
#main #overview > * {
  border: none;
}
#main #thumbs div {
  border: none;
}

/* Insert your own logo (upload separately) */
#logo { 
  height: 80px;
  background: url({{ 'primal7_lrg_logo.png' | asset_url | replace: 'http://','https://' }}) center no-repeat;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-weight: normal !important;
}
label {
  font-weight: normal !important;
}

input {
  font-size: 14px;
}
#header > h1{
  text-align:center;
}

a {
  color: #32c22f;
}

/* Hide the name of the store because we got this nifty logo now */

#header > h1 span.title {
  display:none;
}
#tagline {
  display: none;
}

#container,
#container.slim,
#main,
#container.slim #main,
#header,
#container.slim #header,
#footer,
#container.slim #footer{width:944px;background:#fff;padding:0}

#container {
  margin-top: 0;
  padding: 0 20px;
}

#header {
  padding: 20px 0;
}

#main #overview {
  border-top: 1px solid #222;
  border-bottom: 1px solid #222;
  padding: 10px 0;
}

#products > h2 {
  display: none;
}

#products td {
  font-family: 'AvenirNextLTPro-Regular', sans-serif;
  font-size: 14px;
  color: #222;
  vertical-align: middle;
  padding: 10px 0;
}
#products td div {
  margin-right: 10px;
}

#products .plist-line-price,
#products .plisthead {
  font-family: 'AvenirNextLTPro-Regular', sans-serif;
  font-size: 14px;
  line-height: 2;
  color: #222;
  font-weight: normal;
}

#products .plisthead {
  padding-top: 10px;
}

#wallet {
  width: 280px;
  float: right;
  margin: 0;
}
#cost {
  display: block;
  float: right;
  padding: 20px 92px;
  border: none;
  border-radius: 3px;
  background: #32C22F;
  font: normal normal normal 11px/1 FranklinGothic-DemiCndItal, sans-serif;
  font-size: 20px;
  text-transform: uppercase;
  letter-spacing: 2px;
  color: #FFF;
}

#cost, #order-num {
  text-shadow: none !important;
}
#overview .hint,
#shipping_span,
#steps {
  font-family: 'AvenirNextLTPro-Regular', sans-serif;
  font-size: 14px;
  line-height: 1.5;
  color: #939598;
  margin: 0;
  padding: 0;
  font-style: normal;
  clear: both;
  display: inline-block;
}

#cost #steps {
  line-height: 15px;
}

#content {
  padding: 0;
}

#email {
  padding: 20px 0;
  border-bottom: 1px solid #222;
  background: none !important;
  border: none;
}

#email label {
  margin-bottom: 10px;
  font-family: 'FranklinGothic-DemiCndItal', sans-serif;
  font-size: 20px;
  line-height: 1.5;
  color: #222;
  letter-spacing: 3px;
  font-weight: normal;
}

#content .group {
  background: none !important;
  border: none;
}

#content .group h3 {
  padding: 20px 0 10px;
  font-family: 'FranklinGothic-DemiCndItal', sans-serif;
  font-size: 20px;
  line-height: 1.6;
  color: #222;
  text-align: left;
  border-bottom: none;
  border-top: 1px solid #222;
}

#addresses {
  padding: 0;
}

#addresses label {
  padding: 20px 0 10px;
  font-family: 'AvenirNextLTPro-Regular', sans-serif;
  font-size: 14px;
  line-height: 1.6;
  color: #222;
  text-align: left;
}

#addresses td {
  width: 303px;
  padding-right: 0;
}

#addresses .lbl {
  width: 160px;
  padding: 0;
  vertical-align: middle;
}

#addresses input {
  border-color: #222;
  width: 298px;
}

.address-notification {
  background: none;
  border-color: #222;
  font-family: 'AvenirNextLTPro-Regular', sans-serif;
  font-size: 14px;
  letter-spacing: 1px;
}

#buttons {
  background: none;
  border-top: 1px solid #222;
  border-bottom: none;
  padding: 20px 0 30px 0;
}
#complete-purchase,
#commit-button {
  width: 220px;
  padding: 8px 12px;
  border: none;
  border-radius: 3px;
  background: #32c22f;
  font: normal normal normal 14px/1 FranklinGothic-DemiCndItal, sans-serif;
  letter-spacing: 2px;
  text-transform: uppercase;
  color: #fff;
  font-weight: normal !important;
  cursor: pointer;
}

#complete-purchase:hover,
#commit-button:hover  {
  background: #b3b3b3;
}

#buttons input {
  padding: 16px 12px;
}

#cancel-purchase {
  font-family: 'AvenirNextLTPro-Regular', sans-serif;
  font-size: 14px;
  line-height: 34px;
  letter-spacing: 1px;
  color: #222;
  text-decoration: none;
}

#payment .group {
  padding: 0;
}

#payment .group h3 {
  border-top: none;
  padding: 0;
}

#main p {
  font-family: 'AvenirNextLTPro-Regular', sans-serif;
  font-size: 14px;
  line-height: 1.4;
  letter-spacing: 1px;
}

#payment-method .group {
  padding: 0;
}

#payment-method .group h3 {
  padding-bottom: 0;
}

#payment-method table {
  width: 100%;
}
#payment-method label {
  font-family: 'AvenirNextLTPro-Regular', sans-serif;
  font-size: 14px;
  line-height: 1.6;
}

#payment-method table input {
  border-color: #222;
  width: 100%;
}

#payment-method #credit_card_first_name {
  width: 95%;
}

#payment-method .hint {
  font-family: 'AvenirNextLTPro-Regular', sans-serif;
  font-size: 14px;
  line-height: 1.6;
}

#marketing-box {
  padding: 0;
}


.form td {
  padding-top: 0;
}